{
  "name": "base-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "codegen:fetch": "npx ts-node -T -r  tsconfig-paths/register scripts/codegenGraphql.ts",
    "codegen": "yarn codegen:fetch && graphql-codegen --config codegen.yml",
    "dev": "next dev",
    "build": "yarn install && next build",
